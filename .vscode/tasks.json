{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Example",
			"type": "shell",
			"command": "source /home/phi/esp/v5.5/esp-idf/export.sh && cd ${workspaceFolder}/examples/basic_menu && idf.py build",
			"options": {
				"cwd": "${workspaceFolder}",
				"shell": {
					"executable": "/bin/bash",
					"args": [
						"-lc"
					]
				}
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": {
				"owner": "cpp",
				"fileLocation": [
					"relative",
					"${workspaceFolder}/examples/basic_menu"
				],
				"pattern": {
					"regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			}
		},
